{
  "name": "@mediagoom/node-play",
  "version": "0.0.6",
  "description": "A nodejs streaming server implementation",
  "main": "bin/server/index.js",
  "bin": {
    "nodeplay": "bin/server/index.js"
  },
  "scripts": {
    "testOld": "mocha --reporter spec --timeout 800000 ./src//**/test/index.js ./src//**/test/*.test.js"
    , "test": "mocha ./unit-test//**/*.js"
    , "start": "node ./node_modules/@mediagoom/devman start server",
    "server": "node ./src/server/index.js",
    "stop": "node ./node_modules/@mediagoom/devman stop",
    "install": "node ./dowloadtools.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediagoom/node-play.git"
  },
  "keywords": [
