  "bugs": {
    "url": "https://github.com/mediagoom/node-play/issues"
  },
  "homepage": "https://github.com/mediagoom/node-play#readme",
  "devDependencies": {
    "@mediagoom/devman": "^0.0.6",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "eslint": "^5.7.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "request": "^2.88.0",
    "stream-exhaust": "^1.0.2",
    "url": "^0.11.0"
  },
  "dependencies": {
    "@mediagoom/opflow": "0.0.1",
    "chokidar": "^2.0.4",
